scenario PropTypeInsert

given
  "test.js" = """class UserCard extends React.Component {
  render() {
    return (
      <div>{this.props.name}</div>
    );
  }
}"""

when
  PropTypeInsert

then
  fileContains "test.js" """class UserCard extends React.Component {
  render() {
    return (
      <div>{this.props.name}</div>
    );
  }
}

UserCard.propTypes = {
  name: React.PropTypes.any
};"""
