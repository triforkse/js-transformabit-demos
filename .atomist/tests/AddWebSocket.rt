scenario AddWebSocket

given
  "dummy.js" = """class WebFeed extends React.Component {
  render() {
    return (
      <div>
        <ul>
          {this.props.data.forEach(item => {
            return (<li>{item.data}</li>);
          })}
        </ul>
      </div>
    )
  }
}"""

when
  AddWebSocket component="WebFeed", address="localhost"

then
  fileContains "dummy.js" """class WebFeed extends React.Component {
  constructor() {
    super();
    this.connection = new WebSocket("wss://localhost");
  }

  onError() {}
  onOpen() {}
  onMessage() {}
  render() {
    return (
      <div>
        <ul>
          {this.props.data.forEach(item => {
            return (<li>{item.data}</li>);
          })}
        </ul>
      </div>
    )
  }
}"""
